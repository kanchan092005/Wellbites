<% layout("/layouts/boilerplate") %>

    <link rel="stylesheet" href="/css/search_page_patient.css">

    <div class="search-container">

        <div class="search-inner-container">
            <div class="title">What health concern should we consider for your order?
            </div>
            <div class="dropdown">
                <button onclick="myFunction()" class="dropbtn">Search</button>
                <div id="myDropdown" class="dropdown-content">
                    <input type="text" placeholder="Search.." id="myInput" onkeyup="filterFunction()">
                    <% var displayedDisease=[]; %>
                        <% for(disease of allDisease){ %>
                            <% var diseaseKey=disease.title %>
                                <% if (!displayedDisease.includes(diseaseKey)) { %>
                                    <a href="/<%= disease._id%>/searchedContentPatient">
                                        <%= disease.title %> %>
                                    </a>
                                    <% displayedDisease.push(diseaseKey); %>
                                        <% } %>
                                            <% } %>

                </div>
            </div>
        </div>

        <script src="/js/dropdown.js"></script>